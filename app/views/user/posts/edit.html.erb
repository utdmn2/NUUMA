<h1>投稿編集</h1>
    <%= form_with model: @post, local:true do |f| %>
      <h4>画像</h4>
      <%= f.attachment_field :image %>
      <h4>タイトル</h4>
      <%= f.text_field :title %>
      <h4>本文</h4>
      <%= f.text_area :body %>
    <%= f.submit '更新', class: "btn btn-lg btn-block btn-info" %>
    <% end %>
</div>
